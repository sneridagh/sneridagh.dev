{
  "name": "volto-sneridagh-dev",
  "version": "0.1.0",
  "description": "volto-sneridagh-dev: Volto add-on",
  "main": "src/index.ts",
  "license": "MIT",
  "keywords": [
    "volto-addon",
    "volto",
    "plone",
    "react"
  ],
  "publishConfig": {
    "access": "public"
  },
  "addons": [
    "@eeacms/volto-accordion-block",
    "@eeacms/volto-matomo",
    "@kitconcept/volto-button-block",
    "@kitconcept/volto-heading-block",
    "@kitconcept/volto-highlight-block",
    "@kitconcept/volto-introduction-block",
    "@kitconcept/volto-separator-block",
    "@kitconcept/volto-slider-block",
    "@plonegovbr/volto-code-block",
    "@kitconcept/volto-light-theme"
  ],
  "scripts": {
    "i18n": "rm -rf build/messages && NODE_ENV=production i18n --addon",
    "dry-release": "release-it --dry-run",
    "release": "release-it",
    "release-major-alpha": "release-it major --preRelease=alpha",
    "release-alpha": "release-it --preRelease=alpha"
  },
  "dependencies": {
    "@eeacms/volto-accordion-block": "^10.4.6",
    "@eeacms/volto-matomo": "4.2.1",
    "@kitconcept/volto-button-block": "^2.3.1",
    "@kitconcept/volto-dsgvo-banner": "^2.3.2",
    "@kitconcept/volto-heading-block": "^2.4.0",
    "@kitconcept/volto-highlight-block": "^3.0.1",
    "@kitconcept/volto-introduction-block": "^1.0.0",
    "@kitconcept/volto-light-theme": "3.3.1",
    "@kitconcept/volto-separator-block": "^4.1.1",
    "@kitconcept/volto-slider-block": "^6.3.1",
    "@plone/components": "workspace:*",
    "@plonegovbr/volto-code-block": "2.3.0",
    "classnames": "2.2.6",
    "volto-rss-provider": "^1.0.0"
  },
  "devDependencies": {
    "@plone/scripts": "workspace:*",
    "@plone/types": "workspace:*",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "release-it": "^17.1.1"
  },
  "peerDependencies": {
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-intl": "^3.12.1"
  }
}
